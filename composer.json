{
    "name": "nigo/key-keeper",
    "description": "Application for keep passwords and get it from console",
    "keywords": ["laravel zero", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": ""
        }
    ],
    "require": {
        "php": "^8.2.0",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "illuminate/database": "^11.5",
        "laravel-zero/framework": "^11.0.0",
        "laravel/serializable-closure": "^1.3",
        "nunomaduro/laravel-console-menu": "^3.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "nunomaduro/phpinsights": "^2.11",
        "pestphp/pest": "^2.34.7",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/App",
            "Domain\\": "app/Domain",
            "Support\\": "app/Support",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "build": [
            "php key-keeper app:build",
            "bash -c 'if [ -f .env ]; then cp .env ./builds/.env; fi'"
        ],
        "phpstan": ["vendor/bin/phpstan analyse"],
        "code": ["./vendor/bin/phpinsights"],
        "code-fix": ["./vendor/bin/phpinsights --fix"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "key-keeper"
    ]
}
